{
    "name": "efin3/quizzes",
    "description": "Escola Headles LMS Topic Type GIFT",
    "type": "package",
    "require": {
        "php": ">=7.4",
        "escolalms/auth": "^0",
        "escolalms/categories": "^0",
        "escolalms/core": "^1",
        "escolalms/settings": "^0",
        "escolalms/topic-types": "^0",
        "laravel/framework": ">=8.0",
        "maatwebsite/excel": "^3.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "orchestra/testbench": ">=5.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Maciej Rymarz",
            "email": "maciej.rymarz@escolasoft.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Efin3\\Quizzes\\": "src",
            "Efin3\\Quizzes\\Tests\\": "tests",
            "Efin3\\Quizzes\\Database\\Factories\\": "database/factories",
            "Efin3\\Quizzes\\Database\\Seeders\\": "database/seeders"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Efin3\\Quizzes\\Efin3QuizzesServiceProvider"
            ]
        }
    },
    "config": {
        "optimize-autoloader": false,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
